{
  "name": "sh-market-api",
  "version": "1.0.0",
  "description": "Second-hand market API, allows querying advertisements",
  "main": "index.js",
  "engines": {
    "node": "^7.5.0",
    "npm": "^4.4.0"
  },
  "keywords": [
    "node",
    "babel",
    "flow",
    "eslint"
  ],
  "scripts": {
    "start": "if [[ ${NODE_ENV} == 'production' ]]; then npm run start:prod; else npm run start:dev; fi",
    "start:dev": "nodemon src --exec babel-node -- --dbp 28029",
    "start:prod": "node index.js",
    "build": "babel src -d dist",
    "deploy": "echo \"Would be deploy to server via git to bare repository, but this is DEMO version :)\"",
    "lint": "npm run eslint && npm run flow",
    "eslint": "eslint src",
    "flow": "flow src",
    "clean": "rimraf dist",
    "sloc": "sloc src"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dimpiax/sh-market-api.git"
  },
  "author": "Dima Pilipenko <dimpiax@gmail.com>",
  "license": "ISC",
  "homepage": "https://github.com/dimpiax/sh-market-api#readme",
  "devDependencies": {
    "babel-cli": "6.24.0",
    "babel-eslint": "7.2.1",
    "babel-plugin-transform-class-properties": "6.23.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-env": "1.3.2",
    "babel-preset-flow": "6.23.0",
    "chai": "3.5.0",
    "errorhandler": "1.5.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb-base": "11.1.2",
    "eslint-plugin-flowtype": "2.30.4",
    "eslint-plugin-import": "2.2.0",
    "flow-bin": "0.42.0",
    "flow-remove-types": "1.2.0",
    "linter-flow": "0.1.4",
    "mocha": "3.2.0",
    "nodemon": "1.11.0",
    "rimraf": "2.6.1",
    "sloc": "0.2.0"
  },
  "dependencies": {
    "babel-plugin-transform-runtime": "6.23.0",
    "body-parser": "1.17.1",
    "express": "4.15.2",
    "jsonwebtoken": "7.3.0",
    "method-override": "2.3.8",
    "mongoose": "4.9.2",
    "morgan": "1.8.1",
    "prequest-lite": "0.0.3"
  }
}
